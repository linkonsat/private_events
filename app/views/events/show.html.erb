<% @event.each do |event| %>
    <% if user_signed_in? %>
    <%= event.name %>
    <%= event.description %>
    <%= event.creator %>
    <%= event.event_date %>
    <%= event.event_end_date %>
    
<% end %>
<% end %>
<% @event.first.attendees.each do |attendee| %>
    <%= attendee.user_name %>
    <%= attendee.bio %>
    <%= attendee.email %>
    <% end %> 
<%= button_to "sign up", new_event_attendee_path, params: { :attendee_id => @event.first.user.id, :event_id => @event.first.id }  %>